{
  "name": "kado",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky install",
    "pre-commit": "npm run validate/typescript && npx lint-staged",
    "start": "npx react-native start",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "test": "npx jest",
    "validate/typescript": "npx tsc --noEmit",
    "validate/lint": "npx eslint ./src",
    "prettify": "npx prettier \"./src/**/*\" --write",
    "e2e/build/ios": "npx detox build -c ios.sim.debug",
    "e2e/build/ios/release": "npx detox build -c ios.sim.release",
    "e2e/build/android": "npx detox build -c android.emu.debug",
    "e2e/build/android/release": "npx detox build -c android.emu.release",
    "e2e/test/ios": "npx detox test -c ios.sim.debug",
    "e2e/test/ios/release": "npx detox test -c ios.sim.release",
    "e2e/test/android": "npx detox test -c android.emu.debug",
    "e2e/test/android/release": "npx detox test -c android.emu.release",
    "e2e/test/android-device": "npx detox test -c android.device.debug",
    "e2e/test/android-device/release": "npx detox test -c android.device.release"
  },
